<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_web"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kevin.appfoodpie.WebActivity">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/five_zen">

        <ImageView
            android:id="@+id/aty_web_iv"
            android:src="@mipmap/ic_back_dark_center"
            android:layout_marginTop="@dimen/ten_five_dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:text="@string/sleep"
            android:textSize="@dimen/ten_eight"
            android:layout_marginTop="@dimen/ten_five_dp"
            android:layout_marginLeft="@dimen/five_dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>

    <WebView
        android:id="@+id/aty_web_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>
